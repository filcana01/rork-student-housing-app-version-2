{
  "name": "housing-web",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build:dev": "NODE_ENV=production vite build --mode development",
    "build:sta": "NODE_ENV=production vite build --mode staging",
    "build:prod": "NODE_ENV=production vite build",
    "build": "yarn build:dev; yarn build:sta; yarn build:prod",
    "format": "prettier --ignore-unknown --write .",
    "lint": "eslint --ext .js,.jsx . --fix",
    "i18n-extract": "i18next -c i18next-parser.config.js",
    "i18n-po-export": "i18next-conv -l en -s ./src/locales/en/translation.json -t ./src/locales/en/translation.po && i18next-conv -l it -s ./src/locales/it/translation.json -t ./src/locales/it/translation.po",
    "i18n-po-import": "i18next-conv -l en -s ./src/locales/en/translation.po -t ./src/locales/en/translation.json && i18next-conv -l it -s ./src/locales/it/translation.po -t ./src/locales/it/translation.json",
    "postinstall": "husky",
    "release": "node scripts/release.mjs"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*": "prettier --ignore-unknown  --write"
  },
  "dependencies": {
    "@apollo/client": "^4.0.7",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@fontsource/roboto": "^5.2.8",
    "@hookform/resolvers": "^5.2.2",
    "@mui/icons-material": "^7.3.4",
    "@mui/lab": "7.0.1-beta.18",
    "@mui/material": "^7.3.4",
    "@mui/system": "^7.3.3",
    "@mui/x-data-grid-premium": "^8.14.1",
    "@mui/x-date-pickers-pro": "^8.14.1",
    "@mui/x-license": "^8.14.1",
    "@reduxjs/toolkit": "^2.9.1",
    "@usi-inside/errors": "^5.0.0",
    "@usi-inside/notifications": "^5.0.0",
    "@usi-inside/record-details": "^5.0.0",
    "@usi-inside/utilities": "^5.0.0",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "events": "^3.3.0",
    "file-saver": "^2.0.5",
    "graphql": "^16.11.0",
    "i18next": "^25.6.0",
    "i18next-browser-languagedetector": "^8.2.0",
    "lodash": "^4.17.21",
    "markdown-to-jsx": "^7.7.15",
    "mui-markdown": "^2.0.3",
    "prop-types": "^15.8.1",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-error-boundary": "^6.0.0",
    "react-hook-form": "^7.65.0",
    "react-i18next": "^16.0.1",
    "react-redux": "^9.2.0",
    "react-router": "^7.9.4",
    "rxjs": "^7.8.2",
    "screenfull": "^6.0.2",
    "yup": "^1.7.1",
    "yup-locales": "^1.2.28"
  },
  "devDependencies": {
    "@eslint/js": "^9.37.0",
    "@hookform/devtools": "^4.4.0",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^19.2.2",
    "@vitejs/plugin-react": "^5.0.4",
    "eslint": "^9.37.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^6.1.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "eslint-plugin-unused-imports": "^4.2.0",
    "globals": "^16.4.0",
    "husky": "^9.1.7",
    "i18next-conv": "^15.1.2",
    "i18next-parser": "^9.3.0",
    "lint-staged": "^16.2.4",
    "prettier": "^3.6.2",
    "semver": "^7.7.3",
    "vite": "^7.1.10",
    "vite-plugin-graphql-loader": "^4.0.4",
    "vite-plugin-html": "^3.2.2",
    "vite-plugin-static-copy": "^3.1.4",
    "vite-plugin-svgr": "^4.5.0"
  },
  "packageManager": "yarn@4.10.3"
}
